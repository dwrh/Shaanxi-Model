* Reinventing Fire Baseline Assumptions

sets elec_k(ces) Electricity types excluding transmission /Coal_Power, Oil_Power, Gas_Power, Nuclear, Hydro, Wind, Solar, Biomass /;
alias (elec_k,elec_kk) ;

set nelcoal(elec_k) ;
nelcoal(elec_k) = yes ;
nelcoal("Coal_Power") = no ;
alias (nelcoal, nnelcoal) ;


sets k_primene(ces) Primary energy commodities /Coal, GasDist/,
         j_hvyene(j)    Heavy industries targeted from primary energy reductions /Chemical, Metals, NMetPr /
;
alias (k_primene,kk_primene);

set ncoal(k_primene) ;
ncoal(k_primene) = yes ;
ncoal("Coal") = no ;
alias (ncoal, nncoal) ;

