elsh(elec_k,"2012") = aene(elec_k,"T_D") / sum(elec_kk,aene(elec_kk,"T_D")) ;
elsh("Coal_Power",t)$(ord(t) ge 2) = elsh("Coal_Power","2012")*(1 - coalred*ord(t)/38) ;
elsh(nelcoal,t)$sum(nnelcoal,elsh(nnelcoal,t)) = (1-elsh("Coal_Power",t))*elsh(nelcoal,t)/sum(nnelcoal,elsh(nnelcoal,t)) ;
chk0 = sum((elec_k,t),elsh(elec_k,t)) ;
*display chk0, elsh ;

