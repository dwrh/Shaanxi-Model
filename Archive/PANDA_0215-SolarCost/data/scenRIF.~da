ensh(k_primene,j_hvyene,"2012") = aene(k_primene,j_hvyene) / sum(kk_primene,aene(kk_primene,j_hvyene)) ;

* Use the first ensh equation for exponential decay and the second for linear decay
ensh("Coal",j_hvyene,t)$(ord(t) ge 2) = ensh("Coal",j_hvyene,"2012")*(1 + ((coalred)**(1/38)-1))**ord(t) ;
*ensh("Coal",j_hvyene,t)$(ord(t) ge 2) = ensh("Coal",j_hvyene,"2012")*(1 - coalred*ord(t)/38) ;

ensh(ncoal,j_hvyene,t)$sum(nncoal,ensh(nncoal,j_hvyene,t)) = (1-ensh("Coal",j_hvyene,t))*ensh(ncoal,j_hvyene,t)/sum(nncoal,ensh(nncoal,j_hvyene,t)) ;
chkj(j_hvyene)  = sum((k_primene,t),ensh(k_primene,j_hvyene,t)) ;
display chkj ;


display elsh, ensh ;
